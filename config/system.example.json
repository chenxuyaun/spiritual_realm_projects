{
  "system": {
    "log_level": "INFO",
    "max_cached_models": 3,
    "development_stage": "adult",
    "device": "auto"
  },
  "models": {
    "qwen_chat": {
      "model_path": "Qwen/Qwen-7B-Chat",
      "device": "auto",
      "quantization": null,
      "max_length": 2048,
      "temperature": 0.7
    },
    "gpt2": {
      "model_path": "gpt2",
      "device": "auto",
      "quantization": null,
      "max_length": 1024,
      "temperature": 0.8
    }
  },
  "storage": {
    "vector_db_path": "data/vector_db",
    "chat_history_path": "data/chat_history",
    "consciousness_state_path": ".consciousness/state.json"
  },
  "router": {
    "confidence_threshold": 0.6,
    "enable_logging": true
  },
  "api": {
    "host": "0.0.0.0",
    "port": 8000,
    "enable_auth": false
  }
}
